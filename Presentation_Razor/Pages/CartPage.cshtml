<section class="section section-lg bg-default text-md-left">
    <div class="container">
        <div class="row row-60 justify-content-center">
            <div class="col-lg-8">
                <h4 class="text-spacing-25 text-transform-none">Get in Touch</h4>
                <form action="bat/rd-mailform.php" class="rd-form rd-mailform" data-form-output="form-output-global"
                      data-form-type="contact" method="post">
                    <table class="tCartPageble">
                        <thead>
                        <tr>
                            <th>
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Carts[0].Id)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Carts[0].ProductName)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Carts[0].Quantity)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Carts[0].Price)
                            </th>
                        </tr>
                        </thead>
                        <tbody>
                        @foreach (var item in Model.Carts)
                        {
                            <tr>
                                <td>
                                    <img src="@item.Image"/>
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Id)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ProductName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Quantity)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Price)
                                </td>
                            </tr>
                        }
                        </tbody>
                    </table>
                    <button class="button button-secondary button-winona" type="submit">BUY</button>
                </form>
            </div>
        </div>
